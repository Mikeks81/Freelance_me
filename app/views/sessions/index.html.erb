<h1>Sign In</h1>

<div class="Sing_in">
	<% if flash[:notice] %>
		<%= flash[:notice] %>
	<%end%>
	<%= form_tag sessions_path do %>
		<p><%= label_tag :email %></p>
		<p><%= email_field_tag :email %></p>
		<p><%= label_tag :password %></p>
		<p><%= password_field_tag :password %></p>
		<p><%= submit_tag "Sign In" %></p>
	<% end %>
	<p><%= link_to "Sign Up", users_path %>
</div>




