$('#job_expenses').html("<%= j render(partial: 'expenses/expenses')%>");
$('#expense_box<%= @expense.id %>').hide();
$('#expense_box<%= @expense.id %>').fadeIn();
$('#expense_amount').val('');
$('#expense_date').val('<%= Time.now.strftime("%Y-%m-%d") %>');
